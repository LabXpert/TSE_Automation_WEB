{
  "name": "tseautomationweb",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "web": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "npm --prefix server run dev",
    "server:build": "npm --prefix server run build",
    "server:start": "npm --prefix server run start",
    "reports": "npm --prefix automated-reports run dev",
    "dev": "concurrently -n WEB,API,REPORTS -c auto \"npm:web\" \"npm:server\" \"npm:reports\"",
    "start:full": "concurrently -n WEB,API -c auto \"npm:preview\" \"npm:server:start\""
  },
  "dependencies": {
    "exceljs": "^4.4.0",
    "lucide-react": "^0.540.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-pro-sidebar": "^1.1.0",
    "react-router-dom": "^7.8.0",
    "react-select": "^5.10.2",
    "recharts": "^3.1.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/xlsx": "^0.0.35",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}